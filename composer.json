{
    "name": "endroid/cm-sms",
    "description": "Endroid CM SMS",
    "keywords": ["endroid", "cm", "sms"],
    "homepage": "https://github.com/endroid/cm-sms",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": "^7.1",
        "myclabs/php-enum": "^1.5",
        "psr/http-message": "^1.0",
        "php-http/client-common": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "jms/serializer": "^1.1",
        "ramsey/uuid": "^3.6",
        "symfony/options-resolver": "^2.3|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0|^6.0",
        "guzzlehttp/psr7": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\CmSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\CmSms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
