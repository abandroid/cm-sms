{
    "name": "endroid/cm-sms",
    "description": "Endroid CM SMS",
    "keywords": ["endroid", "cm", "sms", "php"],
    "homepage": "https://github.com/endroid/cm-sms",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "endroid/installer": "^1.0.3",
        "jms/serializer": "^1.1",
        "myclabs/php-enum": "^1.5",
        "ramsey/uuid": "^3.6",
        "symfony/http-client": "^4.3||^5.0",
        "symfony/options-resolver": "^3.4||^4.0||^5.0"
    },
    "require-dev": {
        "endroid/test": "dev-master",
        "guzzlehttp/psr7": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\CmSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\CmSms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
