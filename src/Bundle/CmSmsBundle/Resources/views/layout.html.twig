<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}CM SMS{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ preload(asset('/build/base.css', 'endroid_cm_sms')) }}">
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <div class="container-fluid">
                <div class="row">
                    <main class="col-sm-12 col-md-12 pt-3" role="main">
                        {% block content %}

                        {% endblock %}
                    </main>
                </div>
            </div>
        {% endblock %}
        {% block javascripts %}
            <script src="{{ preload(asset('/build/manifest.js', 'endroid_cm_sms')) }}"></script>
            <script src="{{ preload(asset('/build/base.js', 'endroid_cm_sms')) }}"></script>
        {% endblock %}
    </body>
</html>